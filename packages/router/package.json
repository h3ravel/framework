{
  "name": "@h3ravel/router",
  "version": "1.15.0-alpha.2",
  "description": "Route facade, decorators and controller system for H3ravel.",
  "h3ravel": {
    "providers": [
      "RoutingServiceProvider"
    ]
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./*": "./*"
    }
  },
  "homepage": "https://h3ravel.toneflix.net",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h3ravel/framework.git",
    "directory": "packages/router"
  },
  "keywords": [
    "h3ravel",
    "modern",
    "web",
    "H3",
    "framework",
    "nodejs",
    "typescript",
    "laravel"
  ],
  "scripts": {
    "build": "tsdown --config-loader unconfig",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "test": "jest --passWithNoTests",
    "version-patch": "pnpm version patch"
  },
  "peerDependencies": {
    "@h3ravel/database": "workspace:^0.1.0-alpha.2"
  },
  "dependencies": {
    "h3": "catalog:prod",
    "@h3ravel/contracts": "workspace:^0.1.0-alpha.2",
    "@h3ravel/events": "workspace:^0.1.0-alpha.2",
    "@h3ravel/musket": "catalog:prod",
    "@h3ravel/http": "workspace:^0.1.0-alpha.2",
    "@h3ravel/shared": "workspace:^0.1.0-alpha.2",
    "@h3ravel/support": "workspace:^0.1.0-alpha.2",
    "@h3ravel/foundation": "workspace:^0.1.0-alpha.2",
    "reflect-metadata": "catalog:"
  }
}