{
  "$schema": "node_modules/barrelize/schema.json",
  "semi": false,
  "singleQuote": true,
  "barrels": [
    {
      "name": "index.ts",
      "root": "packages/cache/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/config/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/console/src",
      "exclude": [
        "**/logo.ts",
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/core/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/database/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/filesystem/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/http/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/mail/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/queue/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/router/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/shared/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/hashing/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/view/src",
      "exclude": [
        "**/*copy.ts",
        "**/*.test.ts",
        "**/*.d.ts"
      ],
      "exports": {
        "**/EdgeViewEngine.ts": [
          "*",
          "EdgeViewEngine as ViewManager"
        ],
        "**/Contracts/ViewContract.ts": [
          "type ViewContract"
        ]
      }
    },
    {
      "name": "index.ts",
      "root": "packages/support/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ],
      "exports": {
        "**/Arr.ts": [
          "Arr",
          "/^(?!default$)(.+)/ as $1"
        ],
        "**/Crypto.ts": [
          "* as Crypto",
          "/^(?!default$)(.+)/ as $1"
        ],
        "**/DumpDie.ts": [
          "* as DumpDie",
          "/^(?!default$)(.+)/ as $1"
        ],
        "**/Number.ts": [
          "* as Number",
          "/^(?!default$)(.+)/ as $1"
        ],
        "**/Obj.ts": [
          "* as Obj",
          "/^(?!default$)(.+)/ as $1"
        ],
        "**/Str.ts": [
          "Str",
          "/^(?!default$)(.+)/ as $1"
        ],
        "**/DateTime.ts": [
          "DateTime",
          "/^(?!default$)(.+)/ as $1"
        ]
      }
    }
  ]
}