{
  "$schema": "node_modules/barrelize/schema.json",
  "semi": false,
  "singleQuote": true,
  "barrels": [
    {
      "name": "index.ts",
      "root": "packages/core/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ],
      "exports": {
        "**/H3ravel.ts": [
          "h3ravel"
        ]
      }
    },
    {
      "name": "index.ts",
      "root": "packages/cache/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/config/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/console/src",
      "exclude": [
        "**/prepare.ts",
        "**/fire.ts",
        "**/logo.ts",
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/database/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/filesystem/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/http/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/mail/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/queue/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/events/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/router/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/shared/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/hashing/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/view/src",
      "exclude": [
        "**/*copy.ts",
        "**/*.test.ts",
        "**/*.d.ts"
      ],
      "exports": {
        "**/EdgeViewEngine.ts": [
          "*",
          "EdgeViewEngine as ViewManager"
        ],
        "**/Contracts/ViewContract.ts": [
          "type ViewContract"
        ]
      }
    },
    {
      "name": "index.ts",
      "root": "packages/validation/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/session/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/foundation/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ],
      "order": [
        "Exceptions/HttpException"
      ]
    },
    {
      "name": "index.ts",
      "root": "packages/support/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ],
      "exports": {
        "**/Arr.ts": [
          "Arr",
          "/^(?!default$)(.+)/ as $1"
        ],
        "**/Crypto.ts": [
          "* as Crypto",
          "/^(?!default$)(.+)/ as $1"
        ],
        "**/DumpDie.ts": [
          "/^(?!default$)(.+)/ as $1"
        ],
        "**/Number.ts": [
          "* as Number",
          "/^(?!default$)(.+)/ as $1"
        ],
        "**/Obj.ts": [
          "Obj",
          "/^(?!default$)(.+)/ as $1"
        ],
        "**/Str.ts": [
          "Str",
          "/^(?!default$)(.+)/ as $1"
        ],
        "**/DateTime.ts": [
          "DateTime",
          "/^(?!default$)(.+)/ as $1"
        ]
      }
    },
    {
      "name": "index.ts",
      "root": "packages/contracts/src",
      "exclude": [
        "**/*.test.ts",
        "**/*.d.ts"
      ]
    }
  ]
}