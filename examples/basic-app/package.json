{
  "name": "@h3ravel/example",
  "version": "1.6.2",
  "description": "Example H3ravel App.",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production tsdown --config-loader unconfig -c tsdown.default.config.ts",
    "serve": "tsx watch ./src/server.ts",
    "dev": "NODE_ENV=development pnpm tsdown --config-loader unconfig -c tsdown.default.config.ts",
    "start": "DIST_DIR=dist node -r source-map-support/register dist/server.js",
    "lint": "eslint . --ext .ts",
    "mosket": "node node_modules/@h3ravel/console/bin/fire",
    "postinstall": "node node_modules/@h3ravel/console/bin/prepare"
  },
  "dependencies": {
    "@h3ravel/arquebus": "catalog:prod",
    "@h3ravel/cache": "workspace:^",
    "@h3ravel/config": "workspace:^",
    "@h3ravel/console": "workspace:^",
    "@h3ravel/core": "workspace:^",
    "@h3ravel/database": "workspace:^",
    "@h3ravel/filesystem": "workspace:^",
    "@h3ravel/hashing": "workspace:^",
    "@h3ravel/http": "workspace:^",
    "@h3ravel/mail": "workspace:^",
    "@h3ravel/musket": "catalog:prod",
    "@h3ravel/queue": "workspace:^",
    "@h3ravel/router": "workspace:^",
    "@h3ravel/shared": "workspace:^",
    "@h3ravel/support": "workspace:^",
    "@h3ravel/url": "workspace:^",
    "@h3ravel/view": "workspace:^",
    "@h3ravel/validation": "workspace:^",
    "@h3ravel/foundation": "workspace:^",
    "@h3ravel/session": "workspace:^",
    "cross-env": "catalog:",
    "h3": "catalog:prod",
    "reflect-metadata": "catalog:",
    "source-map-support": "catalog:",
    "sqlite3": "catalog:"
  },
  "devDependencies": {
    "@rollup/plugin-run": "catalog:",
    "@swc/core": "catalog:",
    "@types/node": "^24.9.2",
    "tsdown": "^0.15.12",
    "tsx": "catalog:",
    "typescript": "^5.9.3"
  }
}