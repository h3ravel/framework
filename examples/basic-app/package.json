{
  "name": "@h3ravel/example",
  "version": "1.6.2",
  "description": "Example H3ravel App.",
  "type": "module",
  "private": true,
  "autoload": {
    "namespaces": {
      "App/": "app/",
      "Database/Factories/": "database/factories/",
      "Database/Seeders/": "database/seeders/"
    },
    "files": []
  },
  "scripts": {
    "build": "NODE_ENV=production tsdown --config-loader unconfig -c tsdown.default.config.ts",
    "serve": "tsx watch ./src/server.ts",
    "dev": "NODE_ENV=development pnpm tsdown --config-loader unconfig -c tsdown.default.config.ts",
    "start": "DIST_DIR=dist node -r source-map-support/register dist/server.js",
    "lint": "eslint . --ext .ts",
    "mosket": "node node_modules/@h3ravel/console/bin/fire",
    "postinstall": "node node_modules/@h3ravel/console/bin/prepare"
  },
  "dependencies": {
    "@h3ravel/arquebus": "catalog:prod",
    "@h3ravel/cache": "workspace:^11.1.0-alpha.7",
    "@h3ravel/config": "workspace:^1.5.0-alpha.7",
    "@h3ravel/console": "workspace:^11.15.0-alpha.7",
    "@h3ravel/core": "workspace:^1.22.0-alpha.7",
    "@h3ravel/database": "workspace:^11.5.0-alpha.7",
    "@h3ravel/filesystem": "workspace:^0.4.15-alpha.7",
    "@h3ravel/hashing": "workspace:^0.2.0-alpha.7",
    "@h3ravel/http": "workspace:^11.8.0-alpha.7",
    "@h3ravel/mail": "workspace:^11.0.17-alpha.7",
    "@h3ravel/musket": "catalog:prod",
    "@h3ravel/queue": "workspace:^11.1.0-alpha.7",
    "@h3ravel/router": "workspace:^1.15.0-alpha.7",
    "@h3ravel/shared": "workspace:^0.29.0-alpha.7",
    "@h3ravel/support": "workspace:^0.17.0-alpha.7",
    "@h3ravel/url": "workspace:^1.1.0-alpha.7",
    "@h3ravel/view": "workspace:^0.1.14-alpha.7",
    "@h3ravel/validation": "workspace:^0.1.0-alpha.7",
    "@h3ravel/foundation": "workspace:^0.1.0-alpha.7",
    "@h3ravel/session": "workspace:^0.1.0-alpha.7",
    "@h3ravel/events": "workspace:^0.1.0-alpha.7",
    "cross-env": "catalog:",
    "h3": "catalog:prod",
    "reflect-metadata": "catalog:",
    "source-map-support": "catalog:",
    "sqlite3": "catalog:"
  },
  "devDependencies": {
    "@rollup/plugin-run": "catalog:",
    "@swc/core": "catalog:",
    "@types/node": "^24.9.2",
    "tsdown": "catalog:",
    "tsx": "catalog:",
    "typescript": "^5.9.3"
  }
}